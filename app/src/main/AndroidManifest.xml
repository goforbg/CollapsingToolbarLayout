<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jiahaoliuliu.collapsingtoolbarlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NormalScrollingActivity"
            android:label="@string/screen_name_normal"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".NormalScrollingViewPagerActivity"
            android:label="@string/screen_name_normal_with_view_pager"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".NormalScrollingViewPagerTabActivity"
            android:label="@string/screen_name_normal_with_view_pager_and_tab"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".NormalScrollingViewPagerTabRecyclerViewActivity"
            android:label="@string/screen_name_normal_with_view_pager_tab_and_recycler_view"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>
</manifest>